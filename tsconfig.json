{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": ["src", "custom.d.ts"],
  "exclude": ["src/**/*.spec.ts", "node_modules"],
  "extends": "./tsconfig.paths.json"
}
